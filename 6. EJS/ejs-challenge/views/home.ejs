<%- include('partials/header'); -%> 

<h1>Home</h1>
<p> <%= startContent %> </p> 

<% posts.forEach(function(post) { %>
    <h1> <%= post.title %> </h1>
    <% let content = post.content.substring(0,100) + " ..."; %> 
    <p> <%= content %>
    <a href="/posts/<%= post.title %> " target="_blank"> Read More </a>
    </p>
<% }); %>

<%- include('partials/footer'); -%>  

